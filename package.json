{
  "name": "crypto-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrations:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "migrations:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "migrate": "tsx src/migrate.ts",
    "db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
    "db:seed": "node --loader esbuild-register/loader -r esbuild-register ./src/db/seed.ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@coingecko/cryptoformat": "^0.7.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-table": "^8.10.7",
    "@types/pg": "^8.10.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "lucide-react": "^0.292.0",
    "next": "14.0.3",
    "next-themes": "^0.2.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.6",
    "esbuild-register": "^3.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
